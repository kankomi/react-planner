{
  "name": "planner-react",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "bootstrap": "^4.1.3",
    "classnames": "^2.2.6",
    "jquery": "^3.3.1",
    "moment": "^2.18.1",
    "moment-business": "^3.0.1",
    "popper.js": "^1.14.4",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-scripts": "1.1.5"
  },
  "scripts": {
    "start": "concurrently \"npm:sass:watch\" \"react-scripts start\"",
    "build": "npm sass:build && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "sass:build": "sass-update src/styles/scss:src/styles/css",
    "sass:watch": "sass --watch src/styles/scss:src/styles/css"
  },
  "devDependencies": {
    "concurrently": "^4.0.1",
    "sass": "^1.14.1"
  }
}
